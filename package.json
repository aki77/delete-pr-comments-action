    "@types/node": "^20.17.57",
    "@typescript-eslint/parser": "^8.31.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.28.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^28.12.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.6.2",
    "ts-jest": "^29.3.4",
    "typescript": "^5.7.3"
