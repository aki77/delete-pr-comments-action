{
  "name": "delete-pr-comments-action",
  "version": "2.1.2",
  "private": true,
  "description": "Delete PR review comments by any conditions.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "biome format --write .",
    "format-check": "biome format --check .",
    "lint": "biome check .",
    "lint-fix": "biome check --write .",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aki77/delete-pr-comments-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "aki77",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@types/node": "^20.19.4",
    "@vercel/ncc": "^0.38.3",
    "js-yaml": "^4.1.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@9.4.0"
}
